50 times: [ |x|
    x = x add: 1.

    output = "".
    ((x modulo: 3) equals: 0) ifTrue: [
        output = output concat: "fizz".
    ].
    ((x modulo: 5) equals: 0) ifTrue: [
        output = output concat: "buzz".
    ].
    (output equals: "") ifTrue: [
        output = x.
    ].

    Console println: output.
].
